{
    "name": "wen-vue-list",
    "version": "0.1.0",
    "description": "基于vue2和element2包装的一款多功能列表组件",
    "scripts": {
        "i-all": "npm i & cd example && npm i",
        "start": "start npm run watch & start npm run example",
        "dev": "webpack --config build/webpack.config.dev.js",
        "watch": "webpack --config build/webpack.config.dev.js --watch",
        "example": "cd example && npm run serve",
        "dist": "webpack --config build/webpack.config.prod.js",
        "release": "npm run dist & npm publish"
    },
    "author": "cxw18679850773",
    "license": "MIT",
    "devDependencies": {
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.0.3",
        "webpack": "^4.33.0",
        "webpack-cli": "^3.3.3"
    },
    "main": "./dist/",
    "dependencies": {
        "node-sass": "^4.14.1",
        "sass-loader": "^8.0.0"
    },
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/18679850773/wen-vue-list.git"
    }
}
